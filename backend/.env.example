# Example environment variables for the Jarvis Lead Finder backend
# Copy this file to .env and modify the values as needed

# Database configuration (SQLite by default)
DATABASE_URL=sqlite:///./leads.db

# API configuration
HOST=0.0.0.0
PORT=8000
DEBUG=True

# CORS settings - origins allowed to access the API
# Comma-separated list of origins (no spaces)
CORS_ORIGINS=http://localhost:3000,https://yourappdomain.com 